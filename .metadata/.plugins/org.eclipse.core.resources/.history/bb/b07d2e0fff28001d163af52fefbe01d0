package Backjoon0831;

import java.util.Arrays;
import java.util.Scanner;

public class Main {
	public static void main(String args[]){
		
		Scanner in=new Scanner(System.in);
		
		int[] arr = new int[8];	
		
		for (int i = 0; i < 8; i++) {
			arr[i] = in.nextInt();
		}
		in.close();
		Arrays.sort(arr);
		
	}
}
 