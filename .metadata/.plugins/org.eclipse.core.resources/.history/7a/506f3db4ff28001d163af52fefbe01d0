package Backjoon0831;

import java.util.Arrays;
import java.util.Scanner;

public class Main {
	public static void main(String args[]){
		
		Scanner in=new Scanner(System.in);
		int[] arr = new int[9];	
		
		for (int i = 0; i < 9; i++) {
			arr[i] = in.nextInt();
		}
		
		in.close();
		
		System.out.println(Arrays.toString(arr));
		System.out.println(arr[8]);
	}
}
 