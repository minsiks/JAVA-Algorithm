package Backjoon2;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		int x = in.nextInt();
		int y = in.nextInt();
		
		if (x<0&&y<0) {
			System.out.println("C");
		}else if (x<0&&y>0) {
			System.out.println("B");
		}else if (x>0&&y>0) {
			System.out.println("A");
		}else if (x>0&&y<0) {
			System.out.println("D");
		}in.close();
		
	}

}
